<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="org.openmobster.android.cloud.app"
      android:versionCode="1"
      android:versionName="1.0">
    <application android:label="CloudApp" android:icon="@drawable/icon">
        <activity android:name="org.openmobster.android.cloud.app.App"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        
        <activity android:name="org.openmobster.android.cloud.app.App2"
                  android:label="app2">
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
</manifest> 
